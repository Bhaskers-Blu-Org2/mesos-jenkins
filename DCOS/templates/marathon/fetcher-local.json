{
  "id": "${APP_ID}",
  "cpus": 1,
  "mem": 512,
  "disk": 0,
  "instances": 1,
  "constraints": [
    [
      "os",
      "LIKE",
      "Windows"
    ],
    [
      "hostname",
      "LIKE",
      "${AGENT_HOSTNAME}"
    ]
  ],
  "networks": [ { "mode": "container", "name": "dcosnat" } ],
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "microsoft/iis:windowsservercore-1803",
      "privileged": false,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 31500
        }
      ],
      "forcePullImage": false
    }
  },
  "uris": [
    "file://C:/fetcher-test.zip"
  ],
  "healthChecks": [
    {
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3,
      "port": 80,
      "path": "/",
      "protocol": "MESOS_HTTP",
      "ignoreHttp1xx": false
    }
  ],
  "acceptedResourceRoles": [
    "${AGENT_ROLE}"
  ]
}
